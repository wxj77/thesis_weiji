eprint = {1604.04559},
file = {:Users/weiji/Library/Application Support/Mendeley Desktop/Downloaded/Roberts et al. - 2016 - Dark matter scattering on electrons Accurate calculations of atomic excitations and implications for the DAMA si.pdf:pdf},
issn = {2470-0010},
journal = {Physical Review D},
month = {jun},
number = {11},
pages = {115037},
title = {{Dark matter scattering on electrons: Accurate calculations of atomic excitations and implications for the DAMA signal}},
