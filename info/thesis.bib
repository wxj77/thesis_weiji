@article{Dahl2009,
abstract = {The WIMP limit set by the Xenon10 experiment in 2007 signals a new$\backslash$nera in direct detection of dark matter, with several large-scale$\backslash$nliquid target detectors now under construction. A major challenge$\backslash$nin these detectors will be to understand backgrounds at the level$\backslash$nnecessary to claim a positive WIMP signal. In liquid xenon, these$\backslash$nbackgrounds are dominated by electron recoils, which may be distinguished$\backslash$nfrom the WIMP signal (nuclear recoils) by their higher charge-to-light$\backslash$nratio. During the construction and operation of Xenon10, the prototype$\backslash$ndetector Xed probed the physics of this discrimination. $\backslash$n$\backslash$nParticle interactions in liquid xenon both ionize and excite xenon$\backslash$natoms, giving charge and scintillation signals, respectively. Some$\backslash$nfraction of ions recombine, reducing the charge signal and creating$\backslash$nadditional scintillation. The charge-to-light ratio, determined by$\backslash$nthe initial exciton-ion ratio and the ion recombination fraction,$\backslash$nprovides the basis for discrimination between electron and nuclear$\backslash$nrecoils. Intrinsic fluctuations in the recombination fraction limit$\backslash$ndiscrimination. $\backslash$n$\backslash$nChanges in recombination induce an exact anti-correlation between$\backslash$ncharge and light, and when calibrated this anti-correlation distinguishes$\backslash$nrecombination fluctuations from uncorrelated fluctuations in the$\backslash$nmeasured signals. We determine the mean recombination and recombination$\backslash$nfluctuations as a function of energy and applied field for electron$\backslash$nand nuclear recoils, finding that recombination fluctuations are$\backslash$nalready the limiting factor for discrimination above {\~{}}12 keVr (nuclear$\backslash$nrecoil energy). Below 12 keVr statistical fluctuations in the number$\backslash$nof scintillation photons counted dominate, and we project a x6 improvement$\backslash$nin background rejection with a x2 increase in light collection efficiency.$\backslash$n$\backslash$n$\backslash$nWe also build a simple recombination model that successfully reproduces$\backslash$nthe mean recombination in electron and nuclear recoils, including$\backslash$nthe the surprising reversal of the expected trend for recombination$\backslash$nwith ionization density in low energy electron recoils. The model$\backslash$nalso reproduces the measured recombination fluctuations to within$\backslash$na factor of two at high energies. Surprisingly, the model suggests$\backslash$nthat recombination at low energies is independent of ionization density,$\backslash$nand our observed discrimination is due not to the different stopping$\backslash$npowers of electrons and nuclei as was thought, but rather to a different$\backslash$ninitial exciton-ion ratio. We suggest two possible physical models$\backslash$nfor this new result.},
author = {Dahl, Carl Eric},
file = {:Users/weiji/Library/Application Support/Mendeley Desktop/Downloaded/Dahl - 2009 - The physics of background discrimination in liquid xenon, and first results from Xenon10 in the hunt for WIMP dark matter.pdf:pdf},
number = {September},
title = {{The physics of background discrimination in liquid xenon, and first results from Xenon10 in the hunt for WIMP dark matter.}},
year = {2009}
}
